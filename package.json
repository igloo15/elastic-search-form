    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~9.1.7",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@elastic/elasticsearch": "^7.7.1",
    "@igloo15/elasticsearch-angular-service": "file:./dist/service-lib",
    "@igloo15/elasticsearch-angular-table": "file:./dist/table-lib",
    "@swimlane/ngx-datatable": "^17.0.0",
    "core-js": "^2.6.11",
    "elasticsearch-browser": "^16.7.1",
    "faker": "^4.1.0",
    "ngx-json-viewer": "^2.4.0",
