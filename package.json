{
  "name": "@igloo15/elasticsearch-document-editor",
  "version": "0.4.0-dev.12",
  "description": "A game of taking turns",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "clean": "rimraf dist",
    "serve": "npm run build:service:lib && npm run build:table:lib && ng serve",
    "update:version": "node ./configs/update-version.js",
    "docs": "typedoc --theme markdown --readme docs/README.md --out docs/api src",
    "copy:readme": "cpx ./README.md ./dist/service-lib && cpx ./README.md ./dist/table-lib && cpx ./README.md ./dist/document-lib",
    "git:tag": "cross-var git tag v$npm_package_version",
    "build:lib": "ng build lib",
    "build:document:lib": "ng build document-lib",
    "build:table:lib": "ng build table-lib",
    "build:service:lib": "ng build service-lib",
    "build:lib:prod": "ng build lib --prod",
    "build:lib:watch": "ng build lib --watch",
    "build:app": "ng build app",
    "build:app:prod": "ng build --prod",
    "build:all": "npm run clean && npm run update:version && npm run build:service:lib && npm run build:table:lib && npm run build:document:lib && npm run copy:readme",
    "build": "npm run build:all",
    "deploy:test": "npm run build:all && npm pack --dry-run ./dist/lib",
    "deploy:service": "npm run build:service:lib && npm pack --dry-run ./dist/service-lib",
    "deploy:table": "npm run build:table:lib && npm pack --dry-run ./dist/table-lib",
    "deploy:document": "npm run build:document:lib && npm pack --dry-run ./dist/document-lib",
    "publish:lib:all": "npm publish --access=public ./dist/service-lib && npm publish --access=public ./dist/table-lib && npm publish --access=public ./dist/document-lib",
    "deploy": "npm run build && npm run git:tag && npm run publish:lib:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igloo15/base-angular-lib.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": {
    "name": "igloo15"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igloo15/basic-angular-library/issues"
  },
  "homepage": "https://github.com/igloo15/basic-angular-library#readme",
  "readme": "ERROR: No README data found!",
  "_id": "@igloo15/elasticsearch-document-editor@0.2.0-dev.3",
